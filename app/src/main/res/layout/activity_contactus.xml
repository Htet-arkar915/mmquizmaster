<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#f7f7f7"
    android:layout_height="match_parent"
    tools:context="com.team16.um.mmquizmaster.Navigation.Contactus">
    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:id="@+id/contacttool"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>
    <ImageView
        android:layout_below="@+id/contacttool"
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:src="@drawable/loginlogo"
        android:id="@+id/logo"/>

    <TextView
        android:id="@+id/header"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:layout_below="@+id/logo"
        android:layout_marginTop="30dp"
        android:textSize="25dp"
        android:textColor="@color/colorPrimary"
        android:text="Group Number -UM17/16"
        android:textAppearance="@android:style/TextAppearance.Widget.Button" />
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_below="@+id/header"
        android:layout_height="wrap_content"
        android:id="@+id/rl1">
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:src="@drawable/usericon"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="30dp"
            android:id="@+id/logo1"/>
        <ImageView
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:src="@drawable/symboloffb"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="11dp"/>
    </RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_height="wrap_content"
        android:id="@+id/phone1"
        android:textSize="20dp"
        android:layout_toRightOf="@id/rl1"
        android:layout_marginLeft="16dp"
        android:text="Mg Phyo Kyaw"
        android:textColor="#000000"
        android:layout_below="@+id/header"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_marginTop="8dp"
        android:textSize="16dp"
        android:layout_toRightOf="@id/rl1"
        android:textColor="#747373"
        android:layout_marginLeft="16dp"
        android:layout_height="wrap_content"
        android:text="(CU Meiktila)"
        android:layout_below="@+id/phone1"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ph1"
        android:layout_marginTop="45dp"
        android:layout_marginRight="25dp"
        android:layout_below="@+id/header"
        android:layout_alignParentRight="true"
        android:src="@drawable/ic_call"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl1"
        android:id="@+id/rl2">
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:src="@drawable/usericon"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="60dp"
            android:id="@+id/logo2" />
        <ImageView
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:src="@drawable/symboloffb"
            android:layout_marginTop="54.7dp"
            android:layout_marginLeft="12dp"/>
    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="58dp"
        android:id="@+id/phone2"
        android:layout_toRightOf="@id/rl2"
        android:textSize="20dp"
        android:layout_marginLeft="16dp"
        android:textColor="#000000"
        android:layout_below="@+id/rl1"
        android:text="Ma Win Ngwe Phyo"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_marginTop="8dp"
        android:textSize="16dp"
        android:layout_marginLeft="16dp"
        android:layout_toRightOf="@id/rl2"
        android:textColor="#747373"
        android:layout_height="wrap_content"
        android:text="(CU Meiktila)"
        android:layout_below="@+id/phone2"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ph2"
        android:layout_marginRight="25dp"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/rl1"
        android:layout_marginTop="75dp"
        android:src="@drawable/ic_call"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl2"
        android:id="@+id/rl3">
        <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:src="@drawable/usericon"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="50dp"
            android:id="@+id/logo3" />
        <ImageView
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:src="@drawable/symboloffb"
            android:layout_marginTop="44.5dp"
            android:layout_marginLeft="11.7dp"/>
    </RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:id="@+id/phone3"
        android:textSize="20dp"
        android:layout_toRightOf="@id/rl3"
        android:textColor="#000000"
        android:layout_below="@+id/phone2"
        android:layout_marginLeft="16dp"
        android:text="Mg Htet Arkar Linn"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ph3"
        android:layout_marginRight="25dp"
        android:layout_marginTop="90dp"
        android:layout_below="@+id/phone2"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="180dp"
        android:src="@drawable/ic_call"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_marginTop="8dp"
        android:textSize="16dp"
        android:layout_marginLeft="16dp"
        android:layout_toRightOf="@id/rl1"
        android:textColor="#747373"
        android:layout_height="wrap_content"
        android:text="(CU Meiktila)"
        android:layout_below="@+id/phone3"/>
</RelativeLayout>
